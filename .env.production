# Production Environment Variables
# Ninh ơi, file này chứa config cho production deployment

# App Environment
VITE_APP_ENV=production
VITE_APP_NAME="Kho Vải Tôn"
VITE_APP_VERSION=1.0.0

# Google Drive Configuration

# API Configuration
VITE_API_BASE_URL=https://your-domain.com
VITE_ENABLE_ANALYTICS=true

# Feature Flags
VITE_ENABLE_BATCH_IMPORT=true
VITE_ENABLE_REAL_TIME_SYNC=true

# Performance Settings
VITE_IMAGE_CACHE_TTL=3600
VITE_API_TIMEOUT=10000
VITE_MAX_IMAGE_SIZE=10485760

# Security
VITE_ENABLE_CSP=true
VITE_ALLOWED_ORIGINS=https://your-domain.com
